<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#ffffff</Color>
    <Color x:Key="SecondaryColor">#76e383</Color>
    <Color x:Key="AccentedColor">#498c51</Color>

    <SolidColorBrush x:Key="Primary" Color="{StaticResource PrimaryColor}"/>
    <SolidColorBrush x:Key="Secondary" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="Accented" Color="{StaticResource AccentedColor}"/>

    <SolidColorBrush x:Key="PrimaryHueLightBrush" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="PrimaryHueLightForegroundBrush" Color="{StaticResource Primary200Foreground}"/>
    <SolidColorBrush x:Key="PrimaryHueMidBrush" Color="{StaticResource AccentedColor}"/>
    <SolidColorBrush x:Key="PrimaryHueMidForegroundBrush" Color="{StaticResource Primary500Foreground}"/>
    <SolidColorBrush x:Key="PrimaryHueDarkBrush" Color="{StaticResource SecondaryColor}"/>
    <SolidColorBrush x:Key="PrimaryHueDarkForegroundBrush" Color="{StaticResource Primary700Foreground}"/>
</ResourceDictionary>